	function PimageSelect(image, color, index, swatchObj) {
	var imageID = 'Pimage_' + index;
	var colorDiv = 'Pcolor_' + index;
	var colorInput = 'PcolorInput_' + index;
	var errorMsgDiv = 'Perror_' + index;
	var selectName = 'Pselect_' + index;
	var productForm = 'product_' + index;
	var defImage = 'PdefImage_' + index;
	var defColor = 'PdefColor_' + index;
	var defSwatch = 'PdefSwatch_' + index;
	var formObj=document.forms[productForm];
	var warngMssg='';
	var selectObj = document.getElementById(selectName);
	if(selectObj.selectedIndex==0) {
		warngMssg+='<span class="standard" style="color:#cc0000; font-weight:bold;">* You must fill in size</span><br><br>'
		errorDiv=document.getElementById(errorMsgDiv);
		errorDiv.innerHTML=warngMssg;
	} else {
		document.getElementById(imageID).src=image.src;
		mainColorDiv=document.getElementById(colorDiv);
		mainColorDiv.innerHTML=color;
		eval(defImage).src = image.src;
		document.getElementById(defColor).value = color;
			//reset default swatch border
		if (document.getElementById(defSwatch).value != '') {
			document.getElementById(document.getElementById(defSwatch).value).border = 1;
		}
		document.getElementById(defSwatch).value = swatchObj.id;
		document.getElementById(colorInput).value = color;
		swatchObj.border = 2;
	}
}
	
